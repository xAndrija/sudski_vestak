models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    Uloga: { naziv: string, relationships: { hasMany: Korisnik } }
    Korisnik: { ime_prezime: string, email: string, lozinka: string, uloga_id: id, relationships: { belongsTo: Uloga, hasMany: 'Prodaja Narudzbina' } }
    Kupac: { ime: string, prezime: string, telefon: string, email: 'string nullable', adresa: 'string nullable', relationships: { hasMany: Prodaja } }
    Dobavljac: { naziv: string, kontakt_osoba: string, telefon: string, email: string, relationships: { hasMany: 'Artikal Narudzbina' } }
    Artikal: { naziv: string, opis: text, nabavna_cena: 'decimal:8,2', prodajna_cena: 'decimal:8,2', kolicina_na_stanju: integer, dobavljac_id: id, relationships: { belongsTo: Dobavljac, hasMany: 'StavkaProdaje StavkaNarudzbine' } }
    Prodaja: { datum: date, ukupan_iznos: 'decimal:10,2', nacin_placanja: string, kupac_id: id, korisnik_id: id, relationships: { belongsTo: 'Kupac Korisnik', hasMany: StavkaProdaje } }
    StavkaProdaje: { prodaja_id: id, artikal_id: id, kolicina: integer, cena: 'decimal:8,2', relationships: { belongsTo: 'Prodaja Artikal' } }
    Narudzbina: { datum: date, status: string, dobavljac_id: id, korisnik_id: id, relationships: { belongsTo: 'Dobavljac Korisnik', hasMany: StavkaNarudzbine } }
    StavkaNarudzbine: { narudzbina_id: id, artikal_id: id, kolicina: integer, relationships: { belongsTo: 'Narudzbina Artikal' } }
    Klijent: { ime: string, prezime: string, telefon: string, email: 'string:nullable', adresa: 'string:nullable', relationships: { hasMany: 'Zahtev Poruka' } }
    Zahtev: { broj_zahteva: string, opis: text, tip_vestacenja: string, lokacija: string, hitnost: string, status: string, datum_podnosenja: date, klijent_id: id, relationships: { belongsTo: Klijent, hasOne: TerenskiPodaci } }
    TerenskiPodaci: { datum_terena: date, opis_terena: text, merenja: text, fotografije: text, analize: text, zahtev_id: id, relationships: { belongsTo: Zahtev, hasMany: Dokument } }
    Dokument: { naziv: string, tip: string, putanja: string, datum_dodavanja: date, terenski_podaci_id: id, relationships: { belongsTo: TerenskiPodaci } }
    Izvestaj: { naziv: string, datum: date, status: string, korisnik_id: id, zahtev_id: id, relationships: { belongsTo: 'Korisnik Zahtev' } }
    Poruka: { sadrzaj: text, datum_slanja: date, posiljalac_id: id, primalac_id: id, relationships: { belongsTo: 'Klijent Korisnik' } }
